#blackInterface{display:none; width: 100%; height: 100%; position: absolute; top: 0px; left: 0px; z-index: 200; background-color:rgba(0,0,0,0.4);}
#zx_submitinfo .ishospitals{width: 94%; position: absolute; bottom: 0px; left: 50%; margin-left: -47%; display: none; z-index: 2200;}
#zx_submitinfo .ishospitals .cancel{margin-top: 5px; /*border: 1px solid #c8c7cc;*/ border-radius: 4px;}
#zx_submitinfo .ishospitals .selectItem{border-radius: 0 0 4px 4px;}
#zx_submitinfo .ishospitals .mui-table-view-cell{text-align: center;  padding: 0px 15px; line-height: 43px;}
#zx_submitinfo .ishospitals .mui-table-view:before{display:none;}
#zx_submitinfo .ishospitals .mui-table-view:after{display:none;}
#zx_submitinfo .ishospitals .mui-table-view-cell:after{left:0px;}
#zx_submitinfo .ishospitals .selectAction{display: block; width:55%;margin-left:23%;color:#666;}
#zx_submitinfo .ishospitals .mySelect{ background: url("../resources/imgs/selecte@2x.png") no-repeat center right; color:#EB4853;}
#zx_submitinfo .hospitalTitle{font-size: 17px;line-height: 43px;text-align: center; background: #fff;height: 43px; margin: 0px; padding: 0px;  width: 100%; border-radius: 4px; position: relative; border-bottom-left-radius: 0px; border-bottom-right-radius: 0px;}
#zx_submitinfo .hospitalTitle:after{position: absolute; right: 0;  bottom: 0; left: 0; height: 1px; content: ''; -webkit-transform: scaleY(.5); transform: scaleY(.5); background-color: #c8c7cc;}