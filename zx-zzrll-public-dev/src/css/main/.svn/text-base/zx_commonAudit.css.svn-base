    #zx_commonAudit  .referer_advise{
    /* height: 110px; */
    border: 1px dashed #DCDCDC;
    background-color: #F8F6F6;
    border-radius: 5px;
    line-height: 22px;
    padding-left: 10px;
    margin-top: 15px;
    display: none;
    padding: 5px;
    white-space: normal;
    word-break: break-all;
    word-wrap: break-word;
    }
    /*审批轨迹结束*/
    /*审批意见开始*/
   #zx_commonAudit  .advise{
      	padding-bottom: 15px;
      	margin-bottom: 50px;
    }
  #zx_commonAudit  .advise_input{
   	margin-right: 15px;
   
   }
  #zx_commonAudit  .advise_input textarea{
   	padding: 10px;
   	display:inline-block;
   	border: 1px dashed #DCDCDC;
   	height: 100px;	
   border-radius:5px;
   background-color: #F8F6F6;
   }
