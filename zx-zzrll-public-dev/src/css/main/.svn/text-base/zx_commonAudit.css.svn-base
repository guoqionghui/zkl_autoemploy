	#zx_commonAudit  .route li span,
	#zx_commonAudit .advise_title li span,
	#zx_commonAudit  .decide_button li span{
	  height: 50px;
	  line-height: 50px;
	}
	#zx_commonAudit  .info li:nth-child(1),#zx_commonAudit  .info li:nth-child(2),#zx_commonAudit  .route .route_title li{
	    border-bottom: 1px solid #DCDCDC;
