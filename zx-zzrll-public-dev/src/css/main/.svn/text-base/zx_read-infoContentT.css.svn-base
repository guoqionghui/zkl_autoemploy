
#zx_read-infoContentT{
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

#zx_read-infoContentT .header {
    width: 100%;
    background-color: #ff0000;
    color: #fff;
    
}
#zx_read-infoContentT .contentScroll{
    width: 100%;
    height: 100%;
    overflow-y: scroll;
}

/*阅读资料开始*/
#zx_read-infoContentT .info-content{
	background-color: #fff;	
	padding-left: 15px;
	padding-right: 15px;
	box-sizing: border-box;
	padding-top: 10px;
}
/*确认方框*/
#zx_read-infoContentT .check-confirm{
	margin-top:10px;
	margin-bottom:10px;
}
#zx_read-infoContentT .check-confirm span{	
    content: "";
    vertical-align: middle;
    width: 1rem;
    height: 1rem;
    display: inline-block;
}
#zx_read-infoContentT .check-confirm .readOne{
	background: url(../resources/imgs/checkbox.png);
	background-size: 1rem 1rem;
}
#zx_read-infoContentT .check-confirm .read{
	background: url(../resources/imgs/checked-confirm.png);
	background-size: 1rem 1rem;
}
#zx_read-infoContentT .check-confirm img{
	width:1rem;
	height:1rem;
}
#zx_read-infoContentT .check-confirm .iconfont{
	padding-top:2px;
	font-size:14px;
	font-weight:200;
}
/*阅读资料结束*/