/**
*   该页面主要负责通用样式或者是框架页(index.html)元素的样式
*/

html,body,.mainviewport{
    height: 100%;
    width: 100%;
    margin: 0;
    padding: 0;
    font-size: 100%;
}

/* 隐藏滚动条 */
::-webkit-scrollbar { width: 0; }

.mainviewport>.header{
    position: fixed;
    box-sizing: border-box;
    top:0;
    left:0;
    width:100%;
    /* height:4rem; */
    z-index: 1000;
    text-align: center;
    background-color: #f8f8f8;
    transition:top .15s linear;
    padding: 1rem 0;
    border-bottom: solid 1px #cccccc;
}
.mainviewport>.header img{
	width: 207px;
}

.mainviewport>.header .global-header-pad {
    height: 100%;
    width: 100%;
    background-color: #024b9a;
    color: #fff;
}

.mainviewport>.header .global-header-home{
    float: left;
    width: 2.3rem;
    margin:1rem 0 0 1.6rem;
}

.mainviewport>.header .global-header-back{
    display: none;
    float: left;
    width: 1rem;
    margin:1.2rem 0 0 1.6rem;
}

.mainviewport>.header .global-header-menu{
    display: none;
    float: left;
    width: 1.5rem;
    margin:1.4rem 0rem 0 1.3rem;
}

.mainviewport>.header .global-header-add{
    float: right;
    width: 2rem;
    margin:1rem 1.8rem 0 1.6rem;
}

.mainviewport>.header .header-title{
    position: absolute;
    top:0;
    left: 25%;
    height: 4rem;
    width: 50%;
    line-height: 4rem;
    text-align: center;
    font-size:1.5rem;
    z-index:1;
    margin: 0;
    color:#fff;
}

.mainviewport>.content{
    box-sizing: border-box;
    width:100%;
    height:100%;
    /*background-color: #fff;*/
    /*padding:4rem 0;*/
    overflow: auto;
}
.mainviewport>.footer{
    position: fixed;
    bottom:0;
    left: 0;
    width: 100%;
    z-index: 1000;
    height: 4.0625rem;
    transition:bottom .15s linear;
    background-color: chartreuse;
}

.mainviewport.header-hide>.header{
    top:-6.0625rem;
}

.mainviewport.header-hide>.content{
    padding-top: 0;
}

.mainviewport.footer-hide>.footer{
    bottom:-4.0625rem;
    display:none;
}

.mainviewport.footer-hide>.content{
    padding-bottom:0;

}

.global-shadow{
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 2000;
    top:0;
    left: 0;
}

.global-loading-shadow{
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 8000;
    top: 0;
    left: 0;
}

.global-loading{
    position: fixed;
    width: 12rem;
    height: 7rem;
    margin-left:-6rem;
    margin-top: -3.5rem;
    top: 50%;
    left: 50%;
    z-index: 8001;
    text-align: center;
    border-radius: 0.625rem;
    background-color: #fff;
}

.global-loading img{
	border-radius: 8px;
}

.global-loading>.uil-default-css{
    margin:1.25rem auto;
    left:-1rem;
    top:-0.625rem;
    width: 2.5rem;
    height: 2.5rem;
}

.global-loading>p{
    margin: 0;
    height: 1.875rem;
    line-height: 1.875rem;
    font-size: .6rem;
}

.global-alert-out{
    position: absolute;
    width: 100%;
    height: 100%;
    left:0;
    top:0;
    z-index: 5000;
    overflow: auto;
    display: none;
}

.global-alert-out .global-alert-in{
    position: relative;
    width:30rem;
    background-color: #fff;
    max-width:80%;
    margin: 10rem auto;
    padding-bottom: 3.125rem;
    box-shadow: 0 0 0.625rem rgba(0, 0, 0, .5);
    border-radius: 10px;
}

.global-alert-out .global-alert-in .header{
    height: 3.125rem;
    text-align: center;
    display: none;
}

.global-alert-out .global-alert-in .header .title{
    font-size: 1.5rem;
    height: 3.125rem;
    line-height: 3.125rem;
}

.global-alert-out .global-alert-in .body{
    padding:3rem;
    text-align: left;
    color:#666;
    word-break: break-all;
}

.global-alert-out .global-alert-in .footer{
    position: absolute;
    bottom: 0;
    height: 3.125rem;
    width: 100%;
    text-align: center;
    font-size: 0;
    border-top:1px solid #ccc;
}

.global-alert-out .footer button{
    background: none;
    border: none;
    color: #428BCA;
    display: inline-block;
    width: 45%;
}

.global-alert-out .global-alert-in .footer .btn:focus,.global-alert-out .global-alert-in .footer .btn:active{
    outline: none;
    box-shadow: none;
}

.global-alert-out .global-alert-in .footer .btn{
    padding:0;
    line-height: 3.125rem;
}

.global-alert-out .global-alert-in .footer.cancleHide .cancleBtn{
    display: none;
}

.global-alert-out .global-alert-in .footer.cancleHide .confirmBtn{
    width:100%;
}

.sino-hide{
    display:none;
}

.template{
    display: none !important;
}

