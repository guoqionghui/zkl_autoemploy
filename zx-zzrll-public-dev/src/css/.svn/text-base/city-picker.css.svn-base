/*地址三级联动选择器样式 开始*/
 .city-picker-input {opacity: 0 !important;top: -9999px;left: -9999px;position: absolute;}
 .city-picker-span {position: relative;display: block;outline: 0;-webkit-tap-highlight-color: rgba(0, 0, 0, 0); padding-left:10px;
border-bottom: 1px solid #ccc;background-color: #fff;color: #ccc;cursor: pointer;padding-top:5px;}
 /*.city-picker-span.focus{border-bottom-color: #D93B20;}*/
 .city-picker-span .placeholder{margin-left: 1em; display: inline-block;height: 35px;line-height: 40px;
    border-bottom: 2px solid #EB4853;color:#EB4853;}
 .city-picker-span > .title{margin-left: 1em; }
 .city-picker-span > .title > span {color: #333;/*padding: 5px 0px 11px 0px;*/ margin-right: 15px;height: 35px; display: inline-block;line-height: 40px;}
 .city-picker-dropdown {position: absolute;width: 315px;left: -9999px;top: -9999px;outline: 0;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
z-index: 999999;min-width: 330px;}
 .city-select-wrap {    box-shadow: 0 1px 5px rgba(0, 0, 0, 0.5);}
 .city-select-tab {border-bottom: 1px solid #ccc;background: #f0f0f0;font-size: 13px;display: none}
 .city-select-content {width: 100%;/*min-height: 253.5px;*/background-color: #fff;/*padding: 10px 15px;*/}
 .city-select {font-size: 14px;}
 .city-select dl {line-height: 2;clear: both;padding: 3px 0;margin: 0;}
 .city-select dt {position: absolute;width: 2.5em;font-weight: 500;text-align: right;line-height: 2;left: -50px;}
 .city-select dd {margin-left: 1em; line-height: 2;}
 .city-select a {display: block;padding: 0 10px;outline: 0;text-decoration: none;white-space: nowrap;margin-right: 2px;
text-decoration: none;color: #333;cursor: pointer;}
 .city-select a.active {font-weight:bold; color: #D93B20; border-radius: 2px; background: url(../image/gou.png) no-repeat center right;    margin-right: 20px;}
/* .mui-scroll{transform: translate3d(0px, 0px, 0px) translateZ(0px);}*/
/*地址三级联动选择器样式   结束*/

/*地址选择样式*/
#address,#address2{height: 50%; width: 100%; position: absolute; bottom: 0px; z-index: 999999; display: none;}
#address .chooseAddress,#address2 .chooseAddress{width: 100%; background-color: #fff; text-align: center; /*margin-bottom: -5px;*/position:relative;top:5px;}
#address .chooseAddress h2,#address2 .chooseAddress h2{height: 40px;  font-size: 17px; line-height: 25px;padding-top:14px}
#address .closeBtn,#address2 .closeBtn{position: absolute; right: 0px; top: 2px; color: #D93B20;}
#address .closeBtn .mui-icon,#address2 .closeBtn .mui-icon{font-size: 45px;}
